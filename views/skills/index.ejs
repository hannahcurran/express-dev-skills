<%- include('../partials/header') %>
  <ul>
    <% skills.forEach(function(s) { %>
      <li>
        <form action="/skills/<%= s.id %>?_method=DELETE" method="POST" class="delete-form">
          <button type=" " submit>Delete Skill</button>
        </form>
        <a href="/skills/<%= s.id %>">
          <%= s.skill %>
        </a>
        -
        <%= s.achieved ? 'achieved' : 'still learning' %>
      </li>
      <% }); %>
  </ul>

  <a href="/skills/new">Add New Skill</a>

  <%- include('../partials/footer') %>